    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="529.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
         <children>
            <TextArea fx:id="resultArea" layoutX="23.0" layoutY="27.0" prefHeight="180.0" prefWidth="571.0" />
            <ChoiceBox fx:id="checkCategory" layoutX="646.0" layoutY="27.0" prefWidth="150.0" />
